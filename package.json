{
  "name": "sails-emberjs-responses",
  "description": "A pack to change sails.js responses to ember.js compatible responses",
  "version": "0.0.0",
  "dependencies": {
    "async": "^0.9.0",
    "lodash": "^2.4.1"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint .",
    "checkStyle": "./node_modules/.bin/jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "Alberto Souza <alberto.souza.99@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "mocha": "^1.18.2",
    "istanbul": "^0.2.7",
    "jshint": "^2.5.0",
    "jscs": "^1.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:albertosouza/sails-emberjs-responses.git"
  }
}
